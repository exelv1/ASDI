$(document).ready(function(){
	console.log("I'm rich mofo!");
	$.ajax({
		"url": "_view/divisions",
		"dataType": "json",
		"success": function(data){
			$.each(data.rows, function(index, division){
				var acronym = division.value.acronym;
				var title =   division.value.title;
				$("#divisionList").append(
						$("<li>").append(
								$("<a>").attr("href", "#")
									.text(title)
						)
				);
			});
		}
		
	});
});